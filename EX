      void charger_tab (char*p,char tab[]);
int i=0;
While (p[i]!='\o')
tab[i]=p[i];
i++;

void Inverser tab(char tab[] ,char t[],int m);
int i;
for (i=0;i<m;i++)
t[i]=tab[m-1-i]


